# DuckBug Development Environment Variables
# Copy this file to .env and modify as needed

# API Configuration
API_BASE_URL=http://api.duckbug.localhost

# Database Configuration
POSTGRES_DB=duckbug
POSTGRES_USER=duckbug
POSTGRES_PASSWORD=duckbug
DATABASE_URL=postgres://duckbug:duckbug@postgres:5432/duckbug?sslmode=disable

# Backend Configuration
API_PORT=8080
GOPATH=/go
AIR_WORKSPACE_ROOT=/app

# Frontend Configuration
WDS_SOCKET_PORT=0
VITE_API_BASE_URL=http://api.duckbug.localhost
